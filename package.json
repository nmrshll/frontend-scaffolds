{
  "name": "react-static-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "tailwind:css": "tailwind build src/styles/tailwind.src.css -c tailwind.config.js -o src/styles/tailwind.build.css",
    "start": "npm run tailwind:css && react-static start",
    "stage": "npm run tailwind:css && react-static build --staging",
    "build": "npm run tailwind:css && react-static build",
    "serve": "serve dist -p 3000"
  },
  "dependencies": {
    "autoprefixer": "^9.3.0",
    "axios": "^0.16.2",
    "eslint-config-react-app": "^3.0.4",
    "eslint-plugin-flowtype": "^3.0.0",
    "postcss-cli": "^6.0.1",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^4.3.11",
    "react-router": "^4.2.0",
    "react-static": "^5.5.7",
    "react-svg": "^2.2.16",
    "recompose": "^0.26.0",
    "styled-components": "^3",
    "tailwindcss": "^0.6.6"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-styled-components": "^1.3.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-prettier": "^3.0.0",
    "node-fetch": "^2.2.0",
    "prettier-eslint": "^8.8.1",
    "serve": "^6.1.0"
  },
  "babel": {
    "extends": "react-static/.babelrc",
    "plugins": [
      "babel-plugin-styled-components",
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "src"
        }
      ]
    ]
  }
}
